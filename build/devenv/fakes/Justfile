run-dev:
    docker run --rm -it -v $(pwd):/app -p 9111:9111 ccv-fakes:dev

build-dev:
    cd ../../.. && docker build -f build/devenv/fakes/Dockerfile.dev -t ccv-fakes:dev .

build:
    cd ../../.. && docker build -f build/devenv/fakes/Dockerfile -t ccv-fakes:latest .

clean:
    docker rmi ccv-fakes:latest

clean-dev:
    docker rmi ccv-fakes:dev
